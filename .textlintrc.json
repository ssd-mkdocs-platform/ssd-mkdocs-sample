{
  "rules": {
    "preset-japanese": {
      "no-doubled-joshi": {
        "allow": ["も", "や", "か"]
      }
    },
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": "never"
      },
      "ja-space-around-code": {
        "before": true,
        "after": true
      }
    }
  }
}
